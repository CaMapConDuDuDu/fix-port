{
  "manifest_version": 3,
  "name": "Fix port",
  "version": "1.0",
  "description": "Add the reading time to Chrome Extension documentation articles",
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://localhost/*",
        "https://127.0.0.1/*",
        "http://localhost/*",
        "http://127.0.0.1/*"
      ]
    }
  ]
}
